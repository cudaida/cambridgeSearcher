{
    "manifest_version": 2,
    "name": "Cambridge Dictionary Searcher",
    "description": "This extension will translate selected word by Cambridge Dictionary",
    "version": "1.0",
    "background": { "scripts": ["background.js"] },
    "permissions": [
        "contextMenus",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "minimum_chrome_version": "6.0.0.0",
    "icons": {
        "16": "dictionary.png",
        "48": "dictionary.png",
        "128": "dictionary.png"
    }
}